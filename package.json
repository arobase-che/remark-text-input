{
  "name": "remark-text-input",
  "version": "0.1.0",
  "description": "A remark plugin that parse a syntax for textarea",
  "main": "app.js",
  "dependencies": {
    "unist-util-visit": "^1.1.3"
  },
  "scripts": {
    "pretest": "xo",
    "test": "ava"
  },
  "author": "ache",
  "license": "MIT",
  "keywords": [
    "text",
    "input",
    "textarea",
    "remark"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "rehype-raw": "^2.0.0",
    "rehype-stringify": "^3.0.0",
    "remark-parse": "^5.0.0",
    "remark-rehype": "^3.0.0",
    "unified": "^6.1.6",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
