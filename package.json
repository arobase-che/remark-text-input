{
  "name": "remark-text-input",
  "version": "0.1.0",
  "description": "A remark plugin that parse a syntax for textarea",
  "main": "app.js",
  "dependencies": {
    "unist-util-visit": "^1.1.3"
  },
  "author": "ache",
  "license": "MIT",
  "keywords": [
    "text",
    "input",
    "textarea",
    "remark"
  ],
  "devDependencies": {
    "jest": "^22.1.4",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
